{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es2017"],
    "rootDir": ".",
    "outDir": "../../dist/",
    "module": "commonjs",
    "importHelpers": true,
    "noEmitHelpers": true,
    "declaration": true,
    "noImplicitAny": true,
    "strict": true,
    "removeComments": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "baseUrl": "../"
  },
  "include": [
    "."
  ]
}
